msg=`echo "$msg" | sed 's/:revert:/â³/'`
msg=`echo "$msg" | sed 's/:build:/ðŸ“¦/'`
msg=`echo "$msg" | sed 's/:ci:/ðŸ¤–/'`
msg=`echo "$msg" | sed 's/:docs:/ðŸ“–/'`
msg=`echo "$msg" | sed 's/:feat:/ðŸŒŸ/'`
msg=`echo "$msg" | sed 's/:fix:/ðŸš‘/'`
msg=`echo "$msg" | sed 's/:perf:/âš¡/'`
msg=`echo "$msg" | sed 's/:refactor:/ðŸš§/'`
